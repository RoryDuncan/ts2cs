{
  "name": "ts2cs-transpiler",
  "version": "0.1.0",
  "description": "TypeScript to C# transpiler for Godot 4.x",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "ts2cs": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint . && prettier --check .",
    "format": "prettier --write .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "typescript",
    "csharp",
    "godot",
    "transpiler",
    "source-to-source"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cac": "^6.7.14",
    "ts-morph": "^24.0.0",
    "valibot": "^0.42.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^25.0.9",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
