{
  "name": "ts2cs-transpiler",
  "version": "0.1.0",
  "description": "TypeScript to C# transpiler for Godot 4.x",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ts2cs": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "typescript",
    "csharp",
    "godot",
    "transpiler",
    "source-to-source"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cac": "^6.7.14",
    "ts-morph": "^24.0.0",
    "valibot": "^0.42.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^25.0.9",
    "eslint": "^9.18.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
